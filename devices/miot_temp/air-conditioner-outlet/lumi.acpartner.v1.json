{
  "type": "urn:miot-spec-v2:device:air-conditioner:0000A004:lumi-v1:1",
  "description": "Air Conditioner",
  "services": [
    {
      "iid": 1,
      "type": "urn:miot-spec-v2:service:device-information:00007801:lumi-v1:1",
      "description": "Device Information",
      "properties": [
        {
          "iid": 1,
          "type": "urn:miot-spec-v2:property:manufacturer:00000001:lumi-v1:1",
          "description": "Device Manufacturer",
          "format": "string",
          "access": [
            "read"
          ]
        },
        {
          "iid": 2,
          "type": "urn:miot-spec-v2:property:model:00000002:lumi-v1:1",
          "description": "Device Model",
          "format": "string",
          "access": [
            "read"
          ]
        },
        {
          "iid": 3,
          "type": "urn:miot-spec-v2:property:serial-number:00000003:lumi-v1:1",
          "description": "Device Serial Number",
          "format": "string",
          "access": [
            "read"
          ]
        },
        {
          "iid": 4,
          "type": "urn:miot-spec-v2:property:name:00000004:lumi-v1:1",
          "description": "Device Name",
          "format": "string",
          "access": [
            "read"
          ]
        },
        {
          "iid": 5,
          "type": "urn:miot-spec-v2:property:firmware-revision:00000005:lumi-v1:1",
          "description": "Current Firmware Version",
          "format": "string",
          "access": [
            "read"
          ]
        }
      ]
    },
    {
      "iid": 2,
      "type": "urn:miot-spec-v2:service:air-conditioner:0000780F:lumi-v1:1",
      "description": "Air Conditioner",
      "properties": [
        {
          "iid": 1,
          "type": "urn:miot-spec-v2:property:on:00000006:lumi-v1:1",
          "description": "Switch Status",
          "format": "bool",
          "access": [
            "read",
            "write",
            "notify"
          ]
        },
        {
          "iid": 2,
          "type": "urn:miot-spec-v2:property:mode:00000008:lumi-v1:1",
          "description": "Mode",
          "format": "uint8",
          "access": [
            "read",
            "write",
            "notify"
          ],
          "value-list": [
            {
              "value": 0,
              "description": "Auto"
            },
            {
              "value": 1,
              "description": "Cool"
            },
            {
              "value": 2,
              "description": "Dry"
            },
            {
              "value": 3,
              "description": "Heat"
            },
            {
              "value": 4,
              "description": "Fan"
            }
          ]
        },
        {
          "iid": 3,
          "type": "urn:miot-spec-v2:property:target-temperature:00000021:lumi-v1:1",
          "description": "Target Temperature",
          "format": "float",
          "access": [
            "read",
            "write",
            "notify"
          ],
          "unit": "celsius",
          "value-range": [
            17,
            30,
            1
          ]
        }
      ]
    },
    {
      "iid": 3,
      "type": "urn:miot-spec-v2:service:fan-control:00007809:lumi-v1:1",
      "description": "Fan Control",
      "properties": [
        {
          "iid": 1,
          "type": "urn:miot-spec-v2:property:fan-level:00000016:lumi-v1:1",
          "description": "Fan Level",
          "format": "uint8",
          "access": [
            "read",
            "write",
            "notify"
          ],
          "value-list": [
            {
              "value": 0,
              "description": "Auto"
            },
            {
              "value": 1,
              "description": "Low"
            },
            {
              "value": 2,
              "description": "Medium"
            },
            {
              "value": 3,
              "description": "High"
            }
          ]
        },
        {
          "iid": 2,
          "type": "urn:miot-spec-v2:property:vertical-swing:00000018:lumi-v1:1",
          "description": "Vertical Swing",
          "format": "bool",
          "access": [
            "read",
            "write",
            "notify"
          ]
        }
      ]
    }
  ]
}