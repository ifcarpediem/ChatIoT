{
  "type": "urn:miot-spec-v2:device:diffuser:0000A01E:era-ws01:1",
  "description": "Diffuser",
  "services": [
    {
      "iid": 1,
      "type": "urn:miot-spec-v2:service:device-information:00007801:era-ws01:1",
      "description": "Device Information",
      "properties": [
        {
          "iid": 1,
          "type": "urn:miot-spec-v2:property:manufacturer:00000001:era-ws01:1",
          "description": "Device Manufacturer",
          "format": "string",
          "access": [
            "read"
          ]
        },
        {
          "iid": 2,
          "type": "urn:miot-spec-v2:property:model:00000002:era-ws01:1",
          "description": "Device Model",
          "format": "string",
          "access": [
            "read"
          ]
        },
        {
          "iid": 3,
          "type": "urn:miot-spec-v2:property:serial-number:00000003:era-ws01:1",
          "description": "Device ID",
          "format": "string",
          "access": [
            "read"
          ]
        },
        {
          "iid": 4,
          "type": "urn:miot-spec-v2:property:firmware-revision:00000005:era-ws01:1",
          "description": "Current Firmware Version",
          "format": "string",
          "access": [
            "read"
          ]
        },
        {
          "iid": 5,
          "type": "urn:miot-spec-v2:property:serial-no:000000FF:era-ws01:1",
          "description": "Serial Number",
          "format": "string",
          "access": [
            "read",
            "notify"
          ]
        }
      ]
    },
    {
      "iid": 2,
      "type": "urn:miot-spec-v2:service:diffuser:00007833:era-ws01:1",
      "description": "Diffuser",
      "properties": [
        {
          "iid": 1,
          "type": "urn:miot-spec-v2:property:on:00000006:era-ws01:1",
          "description": "Switch Status",
          "format": "bool",
          "access": [
            "read",
            "write",
            "notify"
          ]
        },
        {
          "iid": 2,
          "type": "urn:miot-spec-v2:property:fault:00000009:era-ws01:1",
          "description": "Device Fault",
          "format": "uint8",
          "access": [
            "read",
            "notify"
          ],
          "value-list": [
            {
              "value": 0,
              "description": "No Faults"
            }
          ]
        },
        {
          "iid": 3,
          "type": "urn:miot-spec-v2:property:mode:00000008:era-ws01:1",
          "description": "Mode",
          "format": "uint8",
          "access": [
            "read",
            "write",
            "notify"
          ],
          "value-list": [
            {
              "value": 0,
              "description": "AnionMode"
            },
            {
              "value": 1,
              "description": "OxygenMode"
            }
          ]
        },
        {
          "iid": 5,
          "type": "urn:miot-spec-v2:property:fan-level:00000016:era-ws01:1",
          "description": "Fan Level",
          "format": "uint8",
          "access": [
            "read",
            "write",
            "notify"
          ],
          "value-list": [
            {
              "value": 1,
              "description": "Level1"
            },
            {
              "value": 2,
              "description": "Level2"
            }
          ]
        }
      ]
    },
    {
      "iid": 6,
      "type": "urn:era-spec:service:custom:00007801:era-ws01:1",
      "description": "custom",
      "properties": [
        {
          "iid": 1,
          "type": "urn:era-spec:property:scene:00000001:era-ws01:1",
          "description": "scene",
          "format": "uint8",
          "access": [
            "write",
            "read",
            "notify"
          ],
          "value-list": [
            {
              "value": 1,
              "description": "Toilet"
            },
            {
              "value": 2,
              "description": "Bedroom"
            },
            {
              "value": 4,
              "description": "Pet"
            },
            {
              "value": 5,
              "description": "Cabinet"
            },
            {
              "value": 6,
              "description": "Custom"
            }
          ]
        },
        {
          "iid": 2,
          "type": "urn:era-spec:property:worktime:00000002:era-ws01:1",
          "description": "worktime",
          "format": "uint16",
          "access": [
            "read",
            "notify",
            "write"
          ],
          "unit": "minutes",
          "value-range": [
            1,
            30,
            1
          ]
        },
        {
          "iid": 3,
          "type": "urn:era-spec:property:sleeptime:00000003:era-ws01:1",
          "description": "sleeptime",
          "format": "uint16",
          "access": [
            "read",
            "notify",
            "write"
          ],
          "unit": "minutes",
          "value-range": [
            30,
            60,
            1
          ]
        },
        {
          "iid": 5,
          "type": "urn:era-spec:property:timer:00000005:era-ws01:1",
          "description": "timer",
          "format": "string",
          "access": []
        },
        {
          "iid": 6,
          "type": "urn:era-spec:property:battery:00000004:era-ws01:1",
          "description": "battery",
          "format": "uint8",
          "access": [
            "notify"
          ],
          "unit": "percentage",
          "value-range": [
            0,
            100,
            1
          ]
        }
      ]
    }
  ]
}