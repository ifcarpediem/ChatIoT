{
  "type": "urn:miot-spec-v2:device:air-monitor:0000A008:zhimi-v1:1",
  "description": "Air Monitor",
  "services": [
    {
      "iid": 1,
      "type": "urn:miot-spec-v2:service:device-information:00007801:zhimi-v1:1",
      "description": "Device Information",
      "properties": [
        {
          "iid": 1,
          "type": "urn:miot-spec-v2:property:manufacturer:00000001:zhimi-v1:1",
          "description": "Device Manufacturer",
          "format": "string",
          "access": [
            "read"
          ]
        },
        {
          "iid": 2,
          "type": "urn:miot-spec-v2:property:model:00000002:zhimi-v1:1",
          "description": "Device Model",
          "format": "string",
          "access": [
            "read"
          ]
        },
        {
          "iid": 3,
          "type": "urn:miot-spec-v2:property:serial-number:00000003:zhimi-v1:1",
          "description": "Device Serial Number",
          "format": "string",
          "access": [
            "read"
          ]
        },
        {
          "iid": 4,
          "type": "urn:miot-spec-v2:property:name:00000004:zhimi-v1:1",
          "description": "Device Name",
          "format": "string",
          "access": [
            "read"
          ]
        },
        {
          "iid": 5,
          "type": "urn:miot-spec-v2:property:firmware-revision:00000005:zhimi-v1:1",
          "description": "Current Firmware Version",
          "format": "string",
          "access": [
            "read"
          ]
        }
      ]
    },
    {
      "iid": 2,
      "type": "urn:miot-spec-v2:service:environment:0000780A:zhimi-v1:1",
      "description": "Environment",
      "properties": [
        {
          "iid": 1,
          "type": "urn:miot-spec-v2:property:pm2.5-density:00000034:zhimi-v1:1",
          "description": "PM2.5 Density",
          "format": "float",
          "access": [
            "read",
            "notify"
          ],
          "value-range": [
            0,
            600,
            1
          ]
        }
      ]
    },
    {
      "iid": 3,
      "type": "urn:miot-spec-v2:service:battery:00007805:zhimi-v1:1",
      "description": "Battery",
      "properties": [
        {
          "iid": 1,
          "type": "urn:miot-spec-v2:property:battery-level:00000014:zhimi-v1:1",
          "description": "Battery Level",
          "format": "uint8",
          "access": [
            "read",
            "notify"
          ],
          "unit": "percentage",
          "value-range": [
            0,
            100,
            1
          ]
        },
        {
          "iid": 2,
          "type": "urn:miot-spec-v2:property:charging-state:00000015:zhimi-v1:1",
          "description": "Charging State",
          "format": "uint8",
          "access": [
            "read",
            "notify"
          ],
          "value-list": [
            {
              "value": 1,
              "description": "Charging"
            },
            {
              "value": 2,
              "description": "Not Charging"
            }
          ]
        }
      ]
    },
    {
      "iid": 4,
      "type": "urn:miot-spec-v2:service:air-monitor:00007812:zhimi-v1:1",
      "description": "Air Monitor",
      "properties": [
        {
          "iid": 1,
          "type": "urn:miot-spec-v2:property:on:00000006:zhimi-v1:1",
          "description": "Switch Status",
          "format": "bool",
          "access": [
            "read",
            "write",
            "notify"
          ]
        }
      ]
    }
  ]
}